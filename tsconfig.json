{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/ts/*",
        "generated/ts/*"
      ]
    },
    "target": "esnext", 
    "module": "esnext",
    "declaration": false,
    "downlevelIteration": false,
